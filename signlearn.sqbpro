lot_x_axis="" unlock_view_pk="_rowid_" freeze_columns="0"><sort/><column_widths><column index="1" value="31"/><column index="2" value="70"/><column index="3" value="300"/><column index="4" value="140"/><column index="5" value="140"/><column index="6" value="140"/><column index="7" value="70"/><column index="8" value="195"/><column index="9" value="70"/><column index="10" value="195"/><column index="11" value="70"/><column index="12" value="195"/><column index="13" value="70"/><column index="14" value="195"/><column index="15" value="90"/></column_widths><filter_values/><conditional_formats/><row_id_formats/><display_formats/><hidden_columns/><plot_y_axes/><global_filter/></table></browse_table_settings></tab_browse><tab_sql><sql name="SQL 1*">-- Insert Modules
INSERT INTO Modules (title, order_index) VALUES
('Module 1: Basics', 1),
('Module 2: Everyday Signs', 2),
('Module 3: Conversations', 3),
('Module 4: Advanced Topics', 4),
('Module 5: Practice &amp; Fluency', 5);

-- Insert Chapters (5 per Module)
INSERT INTO Chapters (module_id, title, description, video_left, video_center, video_right, order_index)
SELECT m.id, 
       'Chapter ' || c || ' of ' || m.title,
       'This is Chapter ' || c || ' of ' || m.title || ', covering essential concepts.',
       'example_video.mp4', 'example_video.mp4', 'example_video.mp4',
       c
FROM Modules m
JOIN (SELECT 1 AS c UNION SELECT 2 UNION SELECT 3 UNION SELECT 4 UNION SELECT 5) AS ch;

-- Insert Questions (10 per Chapter)
INSERT INTO Questions (
    chapter_id, prompt,
    video_left, video_center, video_right,
    choice_a, choice_a_desc,
    choice_b, choice_b_desc,
    choice_c, choice_c_desc,
    choice_d, choice_d_desc,
    correct_choice
)
SELECT c.id,
       'Question ' || q || ' for ' || c.title,
       'example_video.mp4', 'example_video.mp4', 'example_video.mp4',
       'Choice A', 'Explanation for Choice A',
       'Choice B', 'Explanation for Choice B',
       'Choice C', 'Explanation for Choice C',
       'Choice D', 'Explanation for Choice D',
       CASE (q % 4)
           WHEN 1 THEN 'A'
           WHEN 2 THEN 'B'
           WHEN 3 THEN 'C'
           ELSE 'D'
       END
FROM Chapters c
JOIN (SELECT 1 AS q UNION SELECT 2 UNION SELECT 3 UNION SELECT 4 UNION SELECT 5
      UNION SELECT 6 UNION SELECT 7 UNION SELECT 8 UNION SELECT 9 UNION SELECT 10) AS qs;</sql><current_tab id="0"/></tab_sql></sqlb_project>
